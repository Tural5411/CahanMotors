@model CahanMotors.Entities.DTOs.RegisterAddDto

<!-- contact-us-section -->
<section class="contact-us-section v2 layout-radius">
    <div class="boxcar-container">
        <!-- calculater-section -->
        <div class="calculater-sec">
            <div class="right-box">
                <div class="row">
                    <!-- content-column -->
                    <div class="col-lg-12 content-column">
                        <div class="inner-column">
                            <div class="boxcar-title">
                                <h2>Sınaq yürüşünə yazıl</h2>
                                <p>
                                    Test yürüşünə yazılmaq üçün qeydiyyatdan keçin
                                </p>
                            </div>
                            <form class="row" asp-action="Testdrive" asp-controller="Home" method="post">
                                <div class="col-lg-6">
                                    <div class="form_boxes">
                                        <label>Ad Soyad</label>
                                        <input type="text" asp-for="Fullname"  />
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="form_boxes">
                                        <label>Maşının modeli</label>
                                        <input type="text" asp-for="Message"  />
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="form_boxes">
                                        <label>Email</label>
                                        <input type="email" asp-for="Email" />
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="form_boxes">
                                        <label>Əlaqə nömrəsi</label>
                                        <input type="number" asp-for="Number"  />
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="form-submit">
                                        <button type="submit" class="theme-btn v2">
                                            Yazıl<svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 14 14" fill="none">
                                                <g clip-path="url(#clip0_711_3214)">
                                                    <path d="M13.6106 0H5.05509C4.84013 0 4.66619 0.173943 4.66619 0.388901C4.66619 0.603859 4.84013 0.777802 5.05509 0.777802H12.6719L0.113453 13.3362C-0.0384687 13.4881 -0.0384687 13.7342 0.113453 13.8861C0.189396 13.962 0.288927 14 0.388422 14C0.487917 14 0.587411 13.962 0.663391 13.8861L13.2218 1.3277V8.94447C13.2218 9.15943 13.3957 9.33337 13.6107 9.33337C13.8256 9.33337 13.9996 9.15943 13.9996 8.94447V0.388901C13.9995 0.173943 13.8256 0 13.6106 0Z" fill="white" />
                                                </g>
                                                <defs>
                                                    <clipPath id="clip0_711_3214">
                                                        <rect width="14" height="14" fill="white" />
                                                    </clipPath>
                                                </defs>
                                            </svg>
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End calculater-section -->
    </div>
</section>
<!-- contact-us-section -->